{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web/src",
  "projectType": "application",
  "targets": {
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm install",
        "cwd": "apps/web"
      }
    },
    "install:ci": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm ci",
        "cwd": "apps/web"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b && vite build",
        "cwd": "apps/web"
      },
      "outputs": ["{workspaceRoot}/apps/web/dist"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "cwd": "apps/web"
      }
    }
  },
  "tags": ["type:web", "language:typescript"]
}
